{
    "manifest_version": 3,
    "name": "RHprint Helper",
    "description": "API integration for RHprint",
    "version": "1.0",
    "icons": {
        "512": "assets/logo.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "js": [
                "scripts/expose.js"
            ],
            "matches": [
                "https://print.rhit.cf/*",
                "https://print.rose-hulman.edu:9192/*"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "scripts/papercut.js",
                "scripts/rhprint.js",
                "scripts/base64.js",
                "scripts/jquery.min.js"
            ],
            "matches": [
                "https://print.rhit.cf/*",
                "https://print.rose-hulman.edu:9192/*"
            ]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    }
}